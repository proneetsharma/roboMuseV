<launch>

  <include file="$(find robomuse5_navigation)/launch/bringup.launch"/>


  <!--master auto="start"/-->

  <!-- Run the map server -->
  <node name="map_server" pkg="map_server" type="map_server" args="$(find robomuse5_navigation)/maps/playground.yaml" output="screen"/>

  <!--- Run AMCL -->
  <include file="$(find robomuse5_navigation)/launch/amcl.launch"/>
  
  <!--include file="$(find hector_slam_launch)/launch/tutorial.launch"/-->
  <node pkg="rviz" type="rviz" name="rviz"
    args="-d $(find hector_slam_launch)/rviz_cfg/mapping_demo.rviz"/>
  
</launch>

